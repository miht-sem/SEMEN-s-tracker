{
	"name": "semen-s-tracker",
	"displayName": "SEMEN's tracker",
	"description": "Task tracker and timer!!!!",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"publisher": "mihtsem",
	"repository": {
		"type": "git",
		"url": "https://github.com/miht-sem/SEMEN-s-tracker.git"
	},
	"icon": "img/icon.png",
	"author": {
		"name": "Semen Mikhtaniuk",
		"url": "https://miht-sem.space"
	},
	"categories": [
		"Other"
	],
	"main": "out/extension.js",
	"activationEvents": [
		"onCommand:semen-s-tracker.setTask",
		"onCommand:semen-s-tracker.clearTask",
		"onCommand:semen-s-tracker.setTimer",
		"onCommand:semen-s-tracker.toggleTimerActive",
		"onCommand:semen-s-tracker.clearTimer"
	],
	"contributes": {
		"commands": [
			{
				"command": "semen-s-tracker.setTask",
				"title": "Set Current Task"
			},
			{
				"command": "semen-s-tracker.clearTask",
				"title": "Clear Current Task"
			},
			{
				"command": "semen-s-tracker.setTimer",
				"title": "Set Timer"
			},
			{
				"command": "semen-s-tracker.toggleTimerActive",
				"title": "Toggle Timer"
			},
			{
				"command": "semen-s-tracker.clearTimer",
				"title": "Clear Timer"
			}
		]
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.18.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.36"
	},
	"__metadata": {
		"id": "2c7d521f-5398-46f4-94c3-8971ea2f0cc2",
		"publisherDisplayName": "miht.sem",
		"publisherId": "2a4561f9-d212-491c-a601-c2070db4c014"
	}
}